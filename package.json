{
  "name": "oareport",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm run clean && concurrently \"eleventy --serve --port=8080\" \"npm run minify-css -- --watch\" \"npm run minify-js -- --watch\"",
    "clean": "rm -rf dist",
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production npm run minify-css && npm run minify-js",
    "minify-js": "mkdir -p dist/js && esbuild src/js/main.js --bundle --minify --format=esm --target=es2018 --outfile=dist/js/main.min.js",
    "minify-css": "npx tailwindcss -i styles/input.css -c styles/tailwind.config.js -o dist/tailwind.css --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/forms": "^0.5.11",
    "autoprefixer": "^10.4.24",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.2",
    "html-minifier": "^4.0.0",
    "postcss-cli": "^11.0.1",
    "sass": "^1.97.3",
    "tailwindcss": "^3.4.19",
    "terser": "^5.46.0"
  },
  "dependencies": {
    "18": "^0.0.0",
    "@tryghost/content-api": "^1.12.3",
    "axios": "^1.13.4",
    "caniuse-lite": "^1.0.30001766",
    "canvas": "^3.2.1",
    "dotenv": "^17.2.3",
    "feather-icons": "^4.28.0",
    "markdown-it-attrs": "^4.3.1",
    "sal.js": "^0.8.5",
    "sass-loader": "^16.0.6",
    "tailwindcss-font-inter": "^4.0.0"
  }
}
